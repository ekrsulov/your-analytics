{
  "version": 2,
  "builds": [
    {
      "src": "services/website/package.json",
      "use": "vercel-sapper"
    }
  ],
  "routes": [
    {
      "src": "/api/admin/(.*)",
      "dest": "https://admin-api.your-analytics.org/$1"
    },
    {
      "src": "/api/query/(.*)",
      "dest": "https://query-api.your-analytics.org/$1"
    }
  ]
}
